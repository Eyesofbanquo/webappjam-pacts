{
  "consumer": {
    "name": "QizzoConsumer"
  },
  "provider": {
    "name": "QizzoProvider"
  },
  "interactions": [
    {
      "description": "a request for categories",
      "providerState": "there are categories",
      "request": {
        "method": "GET",
        "path": "/api/categories"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "success": true,
          "data": [
            {
              "id": "1",
              "name": "Night",
              "inreview": false
            }
          ]
        },
        "matchingRules": {
          "$.body.data": {
            "min": 1
          },
          "$.body.data[*].*": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}