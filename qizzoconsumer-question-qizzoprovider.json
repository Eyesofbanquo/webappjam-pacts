        "matchingRules": {
          "$.body.data": {
            "min": 1
          },
          "$.body.data[*].*": {
            "match": "type"
          },
          "$.body.data[*].correct_answers": {
            "min": 1
          },
          "$.body.data[*].correct_answers[*].*": {
            "match": "type"
          },
          "$.body.data[*].incorrect_answers": {
            "min": 1
          },
          "$.body.data[*].incorrect_answers[*].*": {
            "match": "type"
          },
          "$.body.data[*].category_uid": {
            "match": "type"
          },
          "$.body.data[*].question_type_uid": {
            "match": "type"
          }
        }
      }
    },
