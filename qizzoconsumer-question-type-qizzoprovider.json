{
  "consumer": {
    "name": "QizzoConsumer-question-type"
  },
  "provider": {
    "name": "QizzoProvider"
  },
  "interactions": [
    {
      "description": "a get request",
      "providerState": "there are question types",
      "request": {
        "method": "GET",
        "path": "/api/question-types"
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": {
          "success": true,
          "data": [
            {
              "id": "0",
              "name": "question-type",
              "deleted": false
            }
          ]
        },
        "matchingRules": {
          "$.body.data": {
            "min": 1
          },
          "$.body.data[*].*": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "a request to create a new question type",
      "providerState": "there are question types",
      "request": {
        "method": "POST",
        "path": "/api/question-types",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "body": {
          "name": "Newer"
        }
      },
      "response": {
        "status": 201,
        "headers": {
        },
        "body": {
          "success": true,
          "data": {
            "id": "0",
            "name": "Newer",
            "deleted": false
          }
        },
        "matchingRules": {
          "$.body.data.id": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "a request to create a question type that already exists",
      "providerState": "there are question types",
      "request": {
        "method": "POST",
        "path": "/api/question-types",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "body": {
          "name": "Random"
        }
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": {
          "success": false,
          "data": null
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}